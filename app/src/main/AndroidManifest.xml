<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.victorhugo.agenciadafaxina">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Agencia da Faxina"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TelaInicial">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TelaPrincipal"
            android:label="Agendando diarista"
            android:parentActivityName=".TelaInicial">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.victorhugo.agenciadafaxina.TelaInicial" />
        </activity>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>
